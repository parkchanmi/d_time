<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="stock">
	<select id="stock_list" parameterType="int" resultType="model.Stock_DTO">
		select * from stock where s_no=#{s_no} order by s_no asc
	</select>
</mapper>




